{
  "name": "fusebar",
  "author": {
    "name": "Daniel F. Dickinson",
    "email": "cshored@thecshore.com",
    "url": "https://www.thecshore.com"
  },
  "version": "0.2.0",
  "description": "Private website search for browsers using Fuse.js as backend",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cshoredaniel/fusebar"
  },
  "homepage": "https://www.thecshore.com/projects/fusebar",
  "keywords": [
    "fuzzy",
    "bitap",
    "search",
    "browser",
    "website",
    "private"
  ],
  "main": "dist/fusebar.js",
  "scripts": {
    "clean": "rimraf ./dist ./work",
    "pretest": "npm run testbuild",
    "test": "jest",
    "premkdirs": "npm run clean",
    "prelint": "npm run mkdirs",
    "prebuild": "npm run lint",
    "pretestbuild": "npm run build",
    "lint": "eslint --max-warnings 0 src",
    "build": "concat -o ./dist/fusebar.js ./src/fusebar-main.js",
    "testbuild": "concat -o ./work/fusebar.test.js ./_vendor/github.com/cshoredaniel/krisk-Fuse/dist/fuse.js ./test/prepare-fuse.js ./dist/fusebar.js ./test/fusebar.test.js",
    "mkdirs": "mkdir -p ./work ./dist",
    "server": "browser-sync start --config bs-config.js"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "concat": "^1.0.3",
    "eslint": "^6.8.0",
    "handlebars": "^4.3.0",
    "jest": "^24.3.1",
    "jsdom": "^16.2.1",
    "mixin-deep": "^1.3.2",
    "rimraf": "^2.6.3",
    "set-value": "^2.0.1"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {}
}
